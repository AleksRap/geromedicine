extends layout/layout
include mixins/cards

block head
    - var title = 'Программы';
    - var pageId = 3;

block header
    include layout/header

block content
    main
        .page-wrapper.container-fluid
            .breadcrumbs
                a(href="index.html") главная
                span /
                span программы


            .amenities
                .wrap-950_pd
                    .wrap-950
                        h2.title.title_gray Программы
                        hr.title__hr.title__hr_width


                        .amenities__head
                            .amenities__pic
                            .amenities__text
                                span.title.title_white патронаж
                                p Lorem ipsum dolor sit amet, consectetur adipiscing elit. Neque viverra semper nunc pellentesque euismod. Arcu ultricies non vel elit. Venenatis leo quam cras ridiculus. Tincidunt faucibus nisl, in libero eget.
                                span.amenities__next подробнее
                                a.amenities__link(href="name-service-patronage.html")

                        p.amenities__description Здесь представлены цены нашего клинического обслуживания, патронаж рассчитывается индивидуально, звоните!


                        .programme
                            .articles.articles_mb
                                section.page-wrapper
                                    h2.title.title_margin Статьи
                                    .articles_img.row
                                        - var time = 0;
                                        .col-md-6.col-12
                                            +article-img('img/article-img-2.jpg','вчера', 'Программа вакцинации', 'programme-vaccine.html')
                                        .col-md-6.col-12
                                            +article-img('img/article-img-1.jpg','21 июля', 'Check-up «К сердцу с любовью»', 'programme-vaccine.html')
                                        .col-md-6.col-12
                                            +article-img('img/article-img-2.jpg','вчера', 'Корпоративное обслуживание для юридических лиц', 'programme-vaccine.html')
                                        .col-md-6.col-12
                                            +article-img('img/article-img-1.jpg','21 июля', 'Check-up «К сердцу с любовью»', 'programme-vaccine.html')




        include components/ask-form



block footer
    include layout/footer